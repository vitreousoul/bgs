{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: gcc build active file",
            "command": "/usr/bin/gcc",
            "args": [
                "-g3",
                "-O0",
                "${workspaceFolder}/chess_bot.c",
                "-o",
                "${workspaceFolder}/chess_bot.out",
                "-DDEBUG=1",
                "-std=c99",
                "-Wall",
                "-Wextra",
                "-Wstrict-prototypes",
                "-Wold-style-definition",
                "-Wmissing-prototypes",
                "-Wmissing-declarations"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}
